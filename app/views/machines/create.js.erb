<% if resource.errors.count > 0 %>
  msg('<ul class="errors"><%=raw resource.errors.to_a.collect{|msg| content_tag :li, msg}.join %></ul>');
<% else %>
  info("保存成功！");
  $("#container").html('<%= escape_javascript(render 'collection') %>');
  typeahead("input[data-provide]");
  editable('<%=change_user_machines_path %>', '输入登录用户');
<% end %>
